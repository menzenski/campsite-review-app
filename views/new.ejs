<% include partials/head %>

<h1>Add a new campground</h1>

<form class="form-inline" action="/campgrounds" method="POST">
    <div class="form-group">
        <label for="yc-new-campground-name">Campground Name</label>
        <input type="text" class="form-control" id="yc-new-campground-name"
            name="name" placeholder="Name">
    </div>
    <div class="form-group">
        <label for="yc-new-campground-image">Campground Image URL</label>
        <input type="text" class="form-control" id="yc-new-campground-image"
            name="image" placeholder="Image URL">
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
</form>

<p><a href="/campgrounds">Go back</a></p>

<% include partials/foot %>
